{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A (very basic) boilerplate for react + redux with react-router and SASS",
  "scripts": {
    "bs": "npm run clean && npm run build && npm run start:prod",
    "build": "cross-env NODE_ENV=production webpack -p --progress --config webpack.config.prod.js",
    "clean": "rimraf dist",
    "lint": "eslint client server",
    "start": "npm run clean && npm run build && npm run start:prod",
    "start:dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js"
  },
  "main": "index.js",
  "author": "Vincent Liaw <vincentcliaw@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.5",
    "async-es": "^2.1.5",
    "axios": "^0.15.3",
    "babel-core": "^6.24.1",
    "body-parser": "^1.16.0",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "cross-env": "^3.1.4",
    "d3": "^3.5.17",
    "d3-array": "^0.8.1",
    "d3-scale": "^0.9.3",
    "d3-shape": "^0.7.1",
    "d3-time-format": "^2.0.3",
    "express": "^4.14.0",
    "helmet": "^3.4.0",
    "leaflet": "1.0.0-rc.2",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0",
    "object-hash": "^1.1.7",
    "radium": "^0.18.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-faux-dom": "^3.0.1",
    "react-fontawesome": "^1.5.0",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "react-split-pane": "^0.1.63",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "webpack": "2.1.0-beta.8",
